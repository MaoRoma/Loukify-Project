{
  "name": "loukify-project",
  "version": "1.0.0",
  "description": "Loukify E-commerce Platform - Full Stack Application",
  "type": "module",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "node --watch server.js",
    "dev:backend": "node --watch server.js",
    "dev:frontend": "cd Frontend && npm run dev",
    "dev:all": "npm run dev:backend & npm run dev:frontend",
    "install:all": "npm install && cd Frontend && npm install",
    "build:frontend": "cd Frontend && npm run build"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.38.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^8.2.1",
    "express-validator": "^7.3.1",
    "multer": "^1.4.5-lts.1"
  },
  "keywords": [
    "express",
    "supabase",
    "backend",
    "api",
    "nextjs",
    "frontend",
    "ecommerce"
  ],
  "author": "Loukify Team",
  "license": "MIT"
}
